#include <iostream>
#include <memory>

using namespace std;

class A{
	shared_ptr<B> b_ptr;
public:
	void set_B(shared_ptr<B> &b){b_ptr = b;};
	A(){"Constructor called";}
	~A(){"Destructor called";}

};

class B{
	shared_ptr<A> a_ptr;
public:
	void set_A(shared_ptr<A> &a){a_ptr = a;};
	A(){"Constructor called";}
	~A(){"Destructor called";}

};
