#ifndef I_PRINTABLE_HPP_
#define I_PRINTABLE_HPP_

class I_Printable{
	friend std::ostream &operator<<(std::ostream &, const IPrintable &);
	public:
		virtual void print(std::ostream &) const =0;
		virtual ~IPrintable(){}
};

#endif /* I_PRINTABLE_HPP_ */
