#include <iostream>
#include "SavingAccount.hpp"
#include "Account.hpp"
#include "Accounts_Util.hpp"
#include <vector>
using namespace std;


int main(){
	vector<Account> Accounts;
	vector<SavingAccount> SavingAccounts;
	vector<TrustAccount> TrustAccounts;
	vector<CheckingAccount> CheckingAccounts;

	Account *ca = new CheckingAccount("Checking Account", 1000);
	Account *sa = new SavingAccount("Saving Account", 1000, 5.00);
	Account *ta = new TrustAccount("Trust Account", 1600, 8);

	cout << * ca << endl;;
	cout << * sa << endl;;
	cout << * ta << endl;;


	return 0;
}




